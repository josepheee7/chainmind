# Blockchain Configuration
REACT_APP_NETWORK=sepolia
REACT_APP_SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
REACT_APP_CHAIN_ID=11155111

# Contract Addresses (will be updated after deployment)
REACT_APP_CHAINMIND_DAO_ADDRESS=0x...
REACT_APP_CHAINMIND_TOKEN_ADDRESS=0x...
REACT_APP_AI_ORACLE_ADDRESS=0x...

# AI Backend Configuration
REACT_APP_AI_BACKEND_URL=http://localhost:8000
